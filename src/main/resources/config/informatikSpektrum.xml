<?xml version="1.0" encoding="UTF-8"?>
<tns:inputSchema xmlns:tns="http://www.example.org/inputSchema"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.example.org/inputSchema inputSchema.xsd ">
	<tns:xpath>
		<tns:expressionWithReplaces>id('volume42')//html:li[{0}]/html:a/@href</tns:expressionWithReplaces>
		<tns:numberStrategy>
			<tns:from>1</tns:from>
			<tns:to>6</tns:to>
		</tns:numberStrategy>
		<tns:next>
			<tns:expressionWithReplaces>id('kb-nav--main')/html:div[3]/html:ol/html:li[{0}]/html:div/html:h3/html:a/@href</tns:expressionWithReplaces>
			<tns:countStrategy>
				<tns:xpath>count(id('kb-nav--main')/html:div[3]/html:ol/html:li)</tns:xpath>
			</tns:countStrategy>
		</tns:next>
	</tns:xpath>

	<tns:titleXpath>concat(substring(//*[@data-test='article-title'], 1, number(exists(//html:body[@class='shared-article-renderer'])) * string-length(//*[@data-test='article-title'])), substring(//*[@class="ArticleTitle"], 1, number(not(exists(//html:body[@class='shared-article-renderer']))) * string-length(//*[@class="ArticleTitle"])))</tns:titleXpath>
	<tns:authorXpath>concat(substring(string-join((//*[@data-test="author-name"]/text()), ', '), 1, number(exists(//html:body[@class='shared-article-renderer'])) * string-length(string-join((//*[@data-test="author-name"]/text()), ', '))), substring(string-join(//html:span[@class="authors__name"]/text(), ', '), 1, number(not(exists(//html:body[@class='shared-article-renderer']))) * string-length(string-join(//html:span[@class="authors__name"]/text(), ', '))))</tns:authorXpath>
	<tns:pageXpath>concat(substring(//html:span[@itemprop="pageStart"]/text(), 1, number(exists(//html:body[@class='shared-article-renderer'])) * string-length(//html:span[@itemprop="pageStart"]/text())), substring(substring-before(substring(//html:span[@class="ArticleCitation_Pages"]/text(), 4), '–'), 1, number(not(exists(//html:body[@class='shared-article-renderer']))) * string-length(substring-before(substring(//html:span[@class="ArticleCitation_Pages"]/text(), 4), '–'))))</tns:pageXpath>
	<tns:volumeXpath>//idonotexist</tns:volumeXpath>
	<tns:categoryXpath>substring-before(concat(substring(//*[@data-test="article-category"]/text(), 1, number(exists(//html:body[@class='shared-article-renderer'])) * string-length(//*[@data-test="article-category"]/text())), substring(//html:span[@class="test-render-category"]/text(), 1, number(not(exists(//html:body[@class='shared-article-renderer']))) * string-length(//html:span[@class="test-render-category"]/text())), ' '), ' ')</tns:categoryXpath>

	<tns:startingHtmlPage>https://link.springer.com/journal/volumesAndIssues/287</tns:startingHtmlPage>
	<tns:baseUrl>https://link.springer.com</tns:baseUrl>
	<tns:sortCategories>true</tns:sortCategories>
</tns:inputSchema>
