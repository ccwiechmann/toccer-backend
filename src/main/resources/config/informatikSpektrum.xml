<?xml version="1.0" encoding="UTF-8"?>
<tns:inputSchema xmlns:tns="http://www.example.org/inputSchema"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.example.org/inputSchema inputSchema.xsd ">
	<tns:xpath>
		<tns:expressionWithReplaces>id('volume41')//html:li[{0}]/html:a/@href</tns:expressionWithReplaces>
		<tns:numberStrategy>
			<tns:from>1</tns:from>
			<tns:to>6</tns:to>
		</tns:numberStrategy>
		<tns:next>
			<tns:expressionWithReplaces>id('kb-nav--main')/html:div[3]/html:ol/html:li[{0}]/html:div/html:h3/html:a/@href</tns:expressionWithReplaces>
			<tns:countStrategy>
				<tns:xpath>count(id('kb-nav--main')/html:div[3]/html:ol/html:li)</tns:xpath>
			</tns:countStrategy>
		</tns:next>
	</tns:xpath>

	<tns:titleXpath>//*[@class="ArticleTitle"]</tns:titleXpath>
	<tns:authorXpath>string-join(//html:span[@class="authors__name"]/text(), ', ')</tns:authorXpath>
	<tns:pageXpath>substring-before(substring(//html:span[@class="ArticleCitation_Pages"]/text(), 4), 'â€“')</tns:pageXpath>
	<tns:volumeXpath>substring(//*[@class="ArticleCitation_Issue"]/text(), 7)</tns:volumeXpath>
	<tns:categoryXpath>substring-before(concat(//html:span[@class="test-render-category"]/text(), ' '), ' ')</tns:categoryXpath>

	<tns:startingHtmlPage>https://link.springer.com/journal/volumesAndIssues/287</tns:startingHtmlPage>
	<tns:baseUrl>https://link.springer.com</tns:baseUrl>
	<tns:sortCategories>true</tns:sortCategories>
</tns:inputSchema>
