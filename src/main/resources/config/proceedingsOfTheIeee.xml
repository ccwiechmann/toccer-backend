<?xml version="1.0" encoding="UTF-8"?>
<tns:inputSchema xmlns:tns="http://www.example.org/inputSchema"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.example.org/inputSchema inputSchema.xsd ">
	<tns:xpath>
		<tns:expressionWithReplaces>id('pi-2016')/html:li[{0}]/html:a/@href</tns:expressionWithReplaces>
		<tns:numberStrategy>
			<tns:from>1</tns:from>
			<tns:to>12</tns:to>
		</tns:numberStrategy>
		<tns:next>
			<tns:multiDataOnOnePageCountStrategy>
				<tns:maxXpath>count(//html:ul[@class="results"]/html:li)</tns:maxXpath>
			</tns:multiDataOnOnePageCountStrategy>
		</tns:next>
	</tns:xpath>

	<tns:titleXpath>normalize-space(string-join(//html:ul[@class="results"]/html:li[{0}]//html:h3//text(), ''))</tns:titleXpath>
	<tns:authorXpath>translate(string-join(//html:ul[@class="results"]/html:li[{0}]//html:div[@class="authors"]//text(), ''), ',', '')</tns:authorXpath>
	<tns:pageXpath>substring(concat(substring-after(normalize-space(string-join(//html:ul[@class="results"]/html:li[{0}]//html:div[@class="txt"]/text(), '')), 'Page(s):'), '-'), 1, string-length(substring-before(concat(substring-after(normalize-space(string-join(//html:ul[@class="results"]/html:li[{0}]//html:div[@class="txt"]/text(), '')), 'Page(s):'), '-'), '-')))</tns:pageXpath>
	
	<tns:volumeXpath>substring-before(normalize-space(substring(substring-after(//html:div[@class="heading"]/html:h2, 'Issue'), 2)), 'â€¢')</tns:volumeXpath>
	<tns:categoryXpath>substring-before(substring-after(translate(string-join(//html:ul[@class="results"]//html:h3/text(), ''), 'ABCDEFGHIJKLMNOPQRSTUVWXYZ', 'abcdefghijklmnopqrstuvwxyz')[contains(., 'of contents')], '['), ']')</tns:categoryXpath>
	<tns:startingHtmlPage>http://ieeexplore.ieee.org/xpl/mostRecentIssue.jsp?punumber=5</tns:startingHtmlPage>
	<tns:baseUrl>http://ieeexplore.ieee.org</tns:baseUrl>
	<tns:sortCategories>false</tns:sortCategories>
</tns:inputSchema>
