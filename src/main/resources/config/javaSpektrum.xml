<?xml version="1.0" encoding="UTF-8"?>
<tns:inputSchema xmlns:tns="http://www.example.org/inputSchema"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.example.org/inputSchema inputSchema.xsd ">
	<tns:xpath>
		<tns:expressionWithReplaces>normalize-space(string-join((//html:div[contains(@class, 'tx-dm-journals')]/html:div[@class='issue'][position()&lt;=6]/html:div[contains(@class, 'w360')]/html:ul/html:li/html:a/@href)[{0}], ''))</tns:expressionWithReplaces>
		<tns:countStrategy>
			<tns:xpath>count(//html:div[contains(@class, 'tx-dm-journals')]/html:div[@class='issue'][position()&lt;=6]/html:div[contains(@class, 'w360')]/html:ul/html:li/html:a/@href)</tns:xpath>
		</tns:countStrategy>
	</tns:xpath>

	<tns:titleXpath>normalize-space(string-join(//html:div[contains(@class, 'w360')]/html:h1/text(), ''))</tns:titleXpath>
	<tns:authorXpath>normalize-space(string-join(//html:div[@class='content']/html:p[2]/text(), ''))</tns:authorXpath>
	
	<tns:categoryXpath>concat(normalize-space(string-join(substring-after(substring-before(//html:div[@class='issueInfo']/text()[1], '/2016'), ' - '), '')), ': ', normalize-space(substring-after(string-join(//html:div[@class='issueInfo']/text(), ''), '2016')))</tns:categoryXpath>
	<tns:startingHtmlPage>http://archive.sigsdatacom.de/fachzeitschriften/javaspektrum/archiv/jahres-uebersicht/2016.html</tns:startingHtmlPage>
	<tns:baseUrl>http://archive.sigsdatacom.de</tns:baseUrl>
	<tns:sortCategories>false</tns:sortCategories>
</tns:inputSchema>
